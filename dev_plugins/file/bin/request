#!/usr/bin/env python3.4
import sys
_version = float(str(sys.version_info[0]) + '.' + str(sys.version_info[1]))

if _version < 3.0:
	#to avoid syntax error in 3.0 +
	exec("print \"Python 3.0 or higher is required\"")
	sys.exit(1)

f = sys.argv[1]
if f.startswith("file://"):
    f = f[7:]

with open(f,'rb') as obj:
    data = obj.read()
    
return data
