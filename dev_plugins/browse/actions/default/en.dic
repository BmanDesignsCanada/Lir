<open,show> [my ]<documents,files>[ folder]
  browse/open "xdg-open `xdg-user-dir DOCUMENTS`"
<open,show> [my ]music[ folder]
  browse/open "xdg-open `xdg-user-dir MUSIC`"
<open,show> [my ]home[ folder]
  browse/open "xdg-open `xdg-user-dir`"
<open,show> [my ]videos[ folder]
  browse/open "xdg-open `xdg-user-dir VIDEOS`"
<open,show> [my ]pictures[ folder]
  browse/open "xdg-open `xdg-user-dir PICTURES`"
<open,show> [my ]downloads[ folder]
  browse/open "xdg-open `xdg-user-dir DOWNLOAD`"

<search,google> (LINE question)
  browse/goto "http://www.google.com/search?q=$question$"
<wolfram,wolf RAM>[ alpha] (LINE question)
  browse/goto "www.wolframalpha.com/input/?i=$question$"
youtube (LINE video)
  browse/goto "https://www.youtube.com/results?search_query=$video$"

where is (LINE place)
  browse/goto "https://maps.google.com/maps?q=$place$&hl=\$LANG"

<open,launch>[ application] (LINE app)
  browse/program "$app$"
